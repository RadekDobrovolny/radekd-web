<header>
    <div class="header-container">
        <div class="xylophone">
            <button data-note="C" class="key" style="background-color: red;"></button>
            <button data-note="D" class="key" style="background-color: orange;"></button>
            <button data-note="E" class="key" style="background-color: yellow;"></button>
            <button data-note="F" class="key" style="background-color: green;"></button>
            <button data-note="G" class="key" style="background-color: blue;"></button>
            <button data-note="A" class="key" style="background-color: indigo;"></button>
            <button data-note="B" class="key" style="background-color: violet;"></button>
        </div>
        <nav>
            <ul>
                {%- for item in collections.page -%}
                <li><a href="{{ item.url }}">{{ item.data.title }}</a></li>
                {%- endfor -%}
            </ul>
        </nav>
    </div>
</header>